<div class="content">
  <h1>Client Management System</h1>
  <hr>
  <h2>Latest Client</h2>
  <% if !@client.nil? %>
    <table class="table table-striped table-bordered">
      <tbody>
        <tr>
          <th>Name</th>
          <td><%= @client.name %></td>
        </tr>
        <tr>
          <th>Contact Number</th>
          <td><%= @client.contact_number %></td>
        </tr>
        <tr>
          <th>Email</th>
          <td><%= @client.email %></td>
        </tr>
      </tbody>
    </table>
  <% end %>
</div>
