<h1>New Contact</h1>
<hr>

<%= form_for(@contact, :url => create_contact_path ) do |f| %>
<h4>First Name</h4>
<%= f.text_field :first_name %>
<h4>Last Name</h4>
<%= f.text_field :last_name %>
<h4>Email</h4>
<%= f.text_field :email %>
<h4>Contact Number</h4>
<%= f.text_field :contact_number %>
<h4>Birthday</h4>
<%= f.date_select :date_of_birth %>
<hr>
<%= f.submit "Save Contact" %>
<% end %>