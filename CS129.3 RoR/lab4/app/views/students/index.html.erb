<div class="content">
  <h1>Student Management System</h1>
  <hr>
  <h2>Latest Student</h2>
  <table class="table">
  	<tr>
  		<th>First Name</th>
  		<th>Last Name</th>
  	</tr>
  	<% @students.each do |student| %>
  		<tr>
  			<td><%= student.first_name %></td>
  			<td><%= student.last_name %></td>
  		</tr>
  	<% end %>
  </table>
</div>
