<%= simple_form_for(user_entry, defaults: { wrapper_html: { class: 'form-group' }, input_html: { class: 'form-control' } }, html: { multipart: true }) do |f| %>
	<%= f.hidden_field :user_id, :value => current_user.id %>
	<%= f.input :title %>
	<%= f.input :description %>
	<%= f.input :photo, as: :file %>
	<%= f.submit "Submit", class: "btn btn-success" %>
<% end %>